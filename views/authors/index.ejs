<h2>all authors</h2>
<form action="/authors" method="get">
    <label for="search">Name</label>
    <input type="text" name="name" id="search" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<% authors.forEach(author =>{ %>
    <div><%= author.name %></div>
<% }) %>

